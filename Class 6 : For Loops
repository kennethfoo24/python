for apple in range(5):
  print (f"Hello World {apple}", apple)

my_names = ["michelle", "kenneth", "daniel"]

for name in my_names:
  print(name)

for new_number in range(5):
  print ((new_number + 1)* 2)
for new_number in range(5):
  print (new_number * 2)

